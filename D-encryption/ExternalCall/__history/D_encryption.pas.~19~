unit D_encryption;

interface

uses Winapi.Windows;

type
  TD_encryption_EncryptDataToFile = procedure(data: string; filename: string; userKey: string);

var
  EncryptDataToFile: TD_encryption_EncryptDataToFile;

implementation

initialization
begin
  EncryptDataToFile := GetProcAddress(LoadSymbol,'EncryptDataToFile');
end;

end.
